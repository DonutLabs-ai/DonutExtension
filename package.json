{
  "name": "donut-extension",
  "description": "Donut Extension",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:style:fix": "stylelint \"**/*.css\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.2",
    "@privy-io/react-auth": "^2.10.0",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-slot": "^1.2.0",
    "@solana/web3.js": "^1.98.2",
    "@web3auth/auth-adapter": "^9.7.0",
    "@web3auth/base": "^9.7.0",
    "@web3auth/ethereum-provider": "^9.7.0",
    "@web3auth/modal": "^9.7.0",
    "@web3auth/modal-react-hooks": "^9.7.0",
    "@web3auth/solana-provider": "^9.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "downshift": "^9.0.9",
    "hotkeys-js": "^3.13.9",
    "lucide-react": "^0.503.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.5.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.8",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/loadable__component": "^5.13.9",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@wxt-dev/module-react": "^1.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "wxt": "^0.20.0"
  }
}
