{
  "manifest_version": 3,
  "name": "Donut Extension",
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
